{
	"geode": "4.9.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "bluescratch.animated_menus",
	"name": "Animated Menus",
	"version": "v1.0.0-beta.1",
	"developer": "Bluescratch",
	"description": "Animates (Some) GD Menus!",
	"dependencies": {
		"geode.node-ids": {
			"importance": "required",
			"version": ">=v1.20.0"
		},
		"geode.custom-keybinds": {
			"version": ">=v1.0.0",
			"importance": "required"
		},
		"bluescratch.main_menu_plus": {
			"version": ">=1.4.1",
			"importance": "recommended"
		}
	},
	"settings": {
		"anim-menus-settings": {
			"type": "title",
			"name": "Animated Menus Settings"
		},
		"enable-mod": {
			"type": "bool",
			"name": "Enable Mod",
			"default": true,
			"description": "Enables or Disables The Mod"
		},
		"speed": {
			"type": "int",
			"name": "Speed",
			"default": 2,
			"control": {
				"arrow-step": 1,
				"arrows": true,
				"big-arrow-step": 2,
				"big-arrows": true,
				"input": true,
				"slider-step": 1,
				"slider": true
			},
			"min": 0,
			"max": 5
		}
	},
	"links": {
		"source": "https://github.com/Bluescratch7545/animated-menus"
	},
	"tags": ["interface","utility","customization"]
}